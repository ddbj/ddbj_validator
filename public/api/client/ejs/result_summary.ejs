<div class="container result-summary <%= status %>">
    <table class="result">
        <tbody>
        <tr>
            <td class="result-title <%= status %>" colspan="2">
                <%= title_message %>
            </td>
        </tr>
        <tr>
            <th>Result</th>
            <td>
                <span class="error_info <%= status %>">
                    <%= result_message %>
                </span>
            </td>
        </tr>
        <tr>
            <th>Files</th>
            <td>
                <% for(key in file_list) { %>
                  <%= key %>: <%= file_list[key] %><br/>
                <% } %>
            </td>
        </tr>
        </tbody>
    </table>
</div>
